template(name='websiteItem')
  .thumbnail.website-item
    img(src="images/website.png" alt="website")
    .caption
      a(href='{{url}}') {{title}}
      p.web-desc {{description}}
      a.btn.btn-default.js-upvote(href='#')
        span.glyphicon.glyphicon-arrow-up(aria-hidden='true')
      a.btn.btn-default.js-downvote(href='#')
        span.glyphicon.glyphicon-arrow-down(aria-hidden='true')
      // you will be putting your up and down vote buttons in here!
